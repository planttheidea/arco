{
  "author": "planttheidea",
  "ava": {
    "concurrency": 5,
    "babel": {
      "babelrc": true,
      "plugins": [
        "transform-decorators-legacy"
      ]
    },
    "failFast": true,
    "files": [
      "test/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "verbose": true
  },
  "bugs": {
    "url": "https://github.com/planttheidea/arco/issues"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "fast-memoize": "^2.0.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-actions": "^1.1.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4"
  },
  "description": "State management sanity with minimal effort",
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "ava": "^0.17.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "css-loader": "^0.26.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-config-rapid7": "^2.6.0",
    "eslint-friendly-formatter": "^2.0.6",
    "eslint-loader": "^1.6.1",
    "html-webpack-plugin": "^2.24.1",
    "in-publish": "^2.0.0",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.4.3",
    "jsdom": "^9.8.3",
    "jsdom-global": "^2.1.0",
    "lodash-webpack-plugin": "^0.10.6",
    "node-uuid": "^1.4.7",
    "normalize.css": "^5.0.0",
    "nyc": "^10.0.0",
    "onchange": "^3.0.2",
    "optimize-js-plugin": "^0.0.4",
    "postcss-loader": "^1.1.1",
    "precommit-hook": "^3.0.0",
    "react-addons-test-utils": "^15.4.1",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^1.16.2"
  },
  "homepage": "https://github.com/planttheidea/arco#readme",
  "keywords": [
    "flux",
    "redux",
    "react",
    "state",
    "store",
    "actions",
    "reducers"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "name": "arco",
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/arco.git"
  },
  "scripts": {
    "build": "NODE_ENV=development webpack --progress --colors",
    "build:minified": "NODE_ENV=production webpack --progress --colors --config=webpack.config.minified.js",
    "clean": "rimraf lib && rimraf dist",
    "dev": "NODE_ENV=development webpack-dashboard -- webpack-dev-server --config=webpack.config.dev.js",
    "docs": "jsdoc -c ./jsdoc.config.json",
    "lint": "NODE_ENV=test eslint src",
    "prepublish": "in-publish && npm run prepublish:compile || echo ''",
    "prepublish:compile": "npm run clean && npm run lint && npm run test:coverage && npm run transpile && npm run build && npm run build:minified",
    "start": "npm run dev",
    "test": "NODE_PATH=. NODE_ENV=test ava --no-cache",
    "test:coverage": "nyc npm test",
    "test:watch": "NODE_PATH=. NODE_ENV=test ava --watch",
    "transpile": "babel src --out-dir lib",
    "validate": "npm ls"
  },
  "version": "0.1.0"
}
