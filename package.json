{
  "author": "planttheidea",
  "ava": {
    "concurrency": 5,
    "babel": {
      "testOptions": {
        "plugins": [
          [
            "@babel/plugin-proposal-decorators",
            {
              "legacy": true
            }
          ]
        ]
      }
    },
    "failFast": true,
    "files": [
      "test/*.js"
    ],
    "require": [
      "@babel/register",
      "@babel/polyfill",
      "./test/helpers/setup-browser-env.js"
    ],
    "verbose": true
  },
  "bugs": {
    "url": "https://github.com/planttheidea/arco/issues"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "immutable": "^3.8.1",
    "lodash": "^4.17.11",
    "moize": "^1.0.3",
    "prop-types": "^15.6.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-actions": "^1.1.0",
    "redux-immutable": "^3.0.8",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4"
  },
  "description": "A React+Redux framework with standards, conventions, and far less boilerplate",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "autoprefixer": "^9.4.7",
    "ava": "^1.0.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "browser-env": "^3.2.5",
    "css-loader": "^2.1.0",
    "enzyme": "^2.6.0",
    "eslint": "^5.13.0",
    "eslint-config-rapid7": "^3.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "in-publish": "^2.0.0",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.4.3",
    "jsdoc-babel": "^0.5.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mock-dom-storage": "^1.0.0",
    "normalize.css": "^8.0.1",
    "nyc": "^13.1.0",
    "onchange": "^5.2.0",
    "optimize-js-plugin": "^0.0.4",
    "postcss-loader": "^3.0.0",
    "precommit-hook": "^3.0.0",
    "react-addons-test-utils": "^15.4.1",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "style-loader": "^0.23.1",
    "uuid": "^3.3.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "homepage": "https://github.com/planttheidea/arco#readme",
  "keywords": [
    "flux",
    "redux",
    "react",
    "state",
    "store",
    "actions",
    "reducers"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "name": "arco",
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/arco.git"
  },
  "scripts": {
    "build": "NODE_ENV=development webpack --progress --colors",
    "build:minified": "NODE_ENV=production webpack --progress --colors --config=webpack.config.minified.js",
    "clean": "rimraf lib && rimraf dist && rimraf docs",
    "dev": "NODE_ENV=development webpack-dev-server --progress --config=webpack.config.dev.js",
    "docs": "jsdoc -c ./jsdoc.config.json && cp img/* docs/img && git add --all",
    "lint": "NODE_ENV=test eslint src",
    "lint:fix": "npm run lint -- --fix",
    "prepublish": "in-publish && npm run prepublish:compile || echo ''",
    "prepublish:compile": "npm run clean && npm run lint && npm run test:coverage && npm run transpile && npm run build && npm run build:minified && npm run docs",
    "start": "npm run dev",
    "test": "NODE_PATH=. NODE_ENV=test ava --no-cache",
    "test:coverage": "nyc npm test",
    "test:watch": "NODE_PATH=. NODE_ENV=test ava --watch",
    "transpile": "babel src --out-dir lib",
    "validate": "npm ls"
  },
  "version": "0.1.3"
}
