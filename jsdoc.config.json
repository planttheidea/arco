{
  "configHelpUrl": "http://usejsdoc.org/about-configuring-jsdoc.html",
  "markdown": {
    "excludeTags": [
      "author"
    ],
    "idInHeadings": true
  },
  "opts": {
    "destination": "./jsdoc",
    "package": "./package.json",
    "readme": "./README.md",
    "recurse": true,
    "template": "./node_modules/ink-docstrap/template",
    "tutorials": "./tutorials"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "include": [
      "src"
    ],
    "exclude": [],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "closure",
      "jsdoc"
    ]
  },
  "templates": {
    "includeDate": false,
    "linenums": true,
    "navType": "inline",
    "outputSourceFiles": true,
    "syntaxTheme": "dark",
    "systemName": "arco",
    "theme": "paper"
  }
}